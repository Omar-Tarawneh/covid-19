<!DOCTYPE html>
<html lang="en">
  <%- include('../layout/head.ejs') %>
  <body>
    <%- include('../layout/header.ejs') %>

    <main class="cards">
      <% covid.forEach(value => { %>
      <div class="card">
        <h1>Date: <%= value.Date %></h1>
        <p>Number of Confirmed Cases: <%= value.Cases %></p>
      </div>
      <% }) %>
    </main>

    <%- include('../layout/footer.ejs') %>
  </body>
</html>
